{
  "name": "osmia",
  "displayName": "osmia",
  "description": "",
  "version": "0.0.8",
  "engines": {
    "vscode": "^1.99.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "osmia.run",
        "title": "osmia: run"
      }
    ],
    "languages": [
      {
        "id": "osmia",
        "extensions": [".osmia"],
        "aliases": ["Osmia", "osmia"],
        "configuration": "./language/osmia-language.json"
      }
    ],
    "grammars": [
      {
        "language": "osmia",
        "scopeName": "source.osmia",
        "path": "./language/osmia.tmLanguage.json"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test",
    "enable-git-hooks": "git config core.hooksPath .githooks",
    "disable-git-hooks": "git config --unset core.hooksPath",
    "prepare-commit": "$EDITOR ./package.json ./package-lock.json && git add -N . && git add -p && git commit"
  },
  "devDependencies": {
    "@types/vscode": "^1.99.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^8.28.0",
    "@typescript-eslint/parser": "^8.28.0",
    "eslint": "^9.23.0",
    "typescript": "^5.8.2",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1"
  }
}
